{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@utils": ["utils/index.ts"],
      "@services": ["services/index.ts"],
      "@types": ["types/index.ts"]
    },
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
